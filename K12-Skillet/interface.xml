<ethernet>
  <entry name="ethernet1/1">
    <layer3>
      <ipv6>
        <neighbor-discovery>
          <router-advertisement>
            <enable>no</enable>
          </router-advertisement>
        </neighbor-discovery>
      </ipv6>
      <ndp-proxy>
        <enabled>no</enabled>
      </ndp-proxy>
      <lldp>
        <enable>no</enable>
      </lldp>
      <dhcp-client/>
    </layer3>
  </entry>
  <entry name="ethernet1/2">
    <layer3>
      <ipv6>
        <neighbor-discovery>
          <router-advertisement>
            <enable>no</enable>
          </router-advertisement>
        </neighbor-discovery>
      </ipv6>
      <ndp-proxy>
        <enabled>no</enabled>
      </ndp-proxy>
      <lldp>
        <enable>no</enable>
      </lldp>
      <ip>
        <entry name="{{ TRUSTED_INTERFACE_IP }}{{ TRUSTED_INTERFACE_SUBNET }}"/>
      </ip>
    </layer3>
  </entry>
</ethernet>
